{
  "name": "x-star-editor",
  "version": "0.2.2",
  "description": "An editor developed by turingstar",
  "license": "MIT",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack && father build && sass src:dist --no-source-map",
    "build:watch": "father dev",
    "dev": "dumi dev",
    "docs:build": "webpack && dumi build",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,scss}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && npm run build",
    "start": "npm run dev"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.22.11",
    "classnames": "^2.3.2",
    "hast-util-to-jsx-runtime": "^1.2.0",
    "rehype-katex": "^6.0.3",
    "rehype-mermaidjs": "^1.0.1",
    "rehype-prism-plus": "^1.6.1",
    "rehype-raw": "^6.1.1",
    "rehype-remark": "^9.1.2",
    "rehype-sanitize": "^5.0.1",
    "rehype-stringify": "^9.0.4",
    "remark-breaks": "^3.0.3",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.3",
    "strip-markdown": "^5.0.1",
    "tinykeys": "^2.1.0",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-typescript": "^7.22.11",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/lint": "^4.0.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-inline-import": "^3.0.0",
    "dumi": "^2.2.0",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.28",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "styled-components": "^6.0.7",
    "stylelint": "^14.9.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "XYD Tech Team tech@xinyoudui.com"
  ]
}
